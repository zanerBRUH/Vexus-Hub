local MiscTab = Window:MakeTab({
    Name = "Misc",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


_G.AdvancedBreakthroughToggle = true

MiscTab:AddToggle({
	Name = "Auto Breakthrough (Advanced)",
	Default = false,
	Callback = function(ToggleAdvancedBreakthrough)
	    
	    while AdvancedBreakthroughToggle do
            AutoBreakthrough()
        end
	end    
})

MiscTab:AddToggle({
	Name = "Auto Breakthrough (Basic)",
	Default = false,
	Callback = function(ToggleBasicBreakthrough)
	    _G.BasicBreakthroughToggle = ToggleBasicBreakthrough
	    wait(1)
	    if BasicBreakthroughToggle == true then
	        local args = {
	    	[1] = true
            	}
	        game:GetService("ReplicatedStorage"):FindFirstChild("Qi Remote Events").Breakthough:FireServer(unpack(args))
        end
	end    
})

MiscTab:AddButton({
	Name = "Breakthrough!",
	Callback = function()
	   while _G.AdvancedBreakthroughToggle == true do
	       print("1")
            AutoBreakthrough()
        end
  	end    
})

